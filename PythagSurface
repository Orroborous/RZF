import math

for j in range(1, 11):
    a_squared = j ** 2
    b_squared = (2 * j) + 1
    c_squared = (j + 1) ** 2
    if a_squared + b_squared == c_squared:
        print(f"{a_squared} + {b_squared} = {c_squared}, Pythagorean triple found!")
    else:
        print(f"{a_squared} + {b_squared} != {c_squared}")
